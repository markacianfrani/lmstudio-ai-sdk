{
  "name": "@cianfrani/lmstudio",
  "version": "0.0.8",
  "description": "LM Studio provider for the AI SDK (Responses API)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:write": "biome format --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "lm-studio",
    "llm",
    "chat",
    "embeddings",
    "responses",
    "reasoning"
  ],
  "author": "Mark Anthony Cianfrani",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.17",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^2.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markacianfrani/lmstudio-ai-sdk"
  },
  "peerDependencies": {
    "ai": "^5.0.0"
  }
}
